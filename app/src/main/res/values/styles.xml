<?xml version="1.0" encoding="utf-8"?>
<resources>
<!--   <item name=""></item>                          -->
    <style name="primary_button" parent="@android:style/Widget.Button">
        <item name="android:background">@null</item>
        <item name="backgroundTint">@color/main_100</item>
        <item name="android:textColor">@color/base_900</item>
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/signika_medium</item>
        <item name="android:letterSpacing">0.08928571428571429</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="secondary_button" parent="@android:style/Widget.Button">
        <item name="android:background">@null</item>
        <item name="backgroundTint">@color/base_0</item>
        <item name="strokeColor">@color/main_100</item>
        <item name="strokeWidth">2dp</item>
        <item name="android:textColor">@color/base_900</item>
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/signika_medium</item>
        <item name="android:letterSpacing">0.08928571428571429</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="text_button" parent="@android:style/Widget.Button">
        <item name="android:background">?android:attr/selectableItemBackground</item>
        <item name="android:textColor">@color/base_900</item>
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/signika_medium</item>
        <item name="android:letterSpacing">0.08928571428571429</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
</resources>